<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProductosMenos">

    <ec.dina.btefonts.TextViewFonts
        android:id="@+id/tvTitutloMenos"
        android:layout_width="307dp"
        android:layout_height="86dp"
        android:layout_marginTop="20dp"
        android:text="@string/productos_menos_costosos"
        android:textColor="#009688"
        android:textSize="24sp"
        app:dm_font="segoe_print"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ec.dina.btefonts.TextViewFonts
        android:id="@+id/tvProdMenos"
        android:layout_width="318dp"
        android:layout_height="537dp"
        android:layout_marginTop="20dp"
        android:text="@string/textview"
        android:textColor="#0B0A0A"
        app:dm_font="segoe_print"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvTitutloMenos" />
</androidx.constraintlayout.widget.ConstraintLayout>